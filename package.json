{
	"name": "har-v",
	"displayName": "har-v",
	"description": "har file viewer/visualiser for vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:harExplorer",
		"onLanguage:har",
		"onLanguage:json",
		"onLanguage:jsonc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "harExplorer",
					"name": "har timeline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/vscode": "^1.67.0",
		"@types/node": "^17.0.39",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"tslint": "^6.1.3",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"jsonc-parser": "^3.0.0"
	}
}
